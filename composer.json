{
    "name": "zeusjammer/inspiring",
    "description": "Alternative inspiring quotes Chuck Norris Facts for Laravel 5",
    "keywords": ["laravel", "package", "console"],
    "license": "GPL-2.0+",
    "authors": [
        {
            "name": "zeusjammer",
            "email": "zeusjammer@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "^5"
    },
    "autoload": {
        "psr-4": {
            "Zeusjammer\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^4 || ^5"
    },
    "autoload-dev": {
        "psr-4": {
            "Zeusjammer\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "tests": [
            "php vendor/bin/phpunit"
        ]
    }
}
